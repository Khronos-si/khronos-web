{
    "extends": ["airbnb-base", "prettier"],
    "plugins": ["prettier", "node"],
    "rules": {
      "prettier/prettier": "error",
      "no-unused-vars": "warn",
      "no-console": "warn",
      "func-names": "off",
      "no-process-exit": "off",
      "object-shorthand": "off",
      "class-methods-use-this": "off",
      "no-underscore-dangle": [1, { "allow": ["_id"] }],
      "eqeqeq": "error",
      "no-restricted-syntax": "off",
      "no-await-in-loop": "off"
    }
  }